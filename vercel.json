{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "^/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "^/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ]
  }